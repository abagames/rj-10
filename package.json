{
  "name": "rj-10",
  "version": "1.0.0",
  "description": "Text tilemap based game development environment",
  "scripts": {
    "watch": "parcel src/demo/index.html -d tmp",
    "build": "parcel build src/demo/index.html -d docs --no-source-maps --public-url ./",
    "watch_tutorial": "parcel src/demo/tutorial.html -d tmp",
    "build_tutorial": "parcel build src/demo/tutorial.html -d docs --no-source-maps --public-url ./",
    "build_lib": "parcel build src/main.ts -d docs/build --no-source-maps --global rj10"
  },
  "author": "abagames",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "0.0.6",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "gif-capture-canvas": "^1.0.1",
    "mml-iterator": "^1.1.0",
    "tone": "^13.4.9"
  },
  "browserslist": [
    "last 1 Chrome version"
  ]
}
